{% for item in post.get_field('flex_sections') %}
    {% if item.acf_fc_layout == 'header' %}
    	{% include "flex_sections/header.twig" %}
    {% elseif item.acf_fc_layout == 'body_content' %}
    	{% include "flex_sections/body_content.twig" %}
    {% elseif item.acf_fc_layout == 'image_carousel' %}
    	{% include "flex_sections/image_carousel.twig" %}
    {% elseif item.acf_fc_layout == 'cta' %}
    	{% include "flex_sections/cta.twig" %}
    {% elseif item.acf_fc_layout == 'item_grid' %}
    	{% include "flex_sections/item_grid.twig" %}
    {% endif %}
{% endfor %}